{
  "name": "@iume/react-native-spinkit",
  "version": "2.0.2",
  "scripts": {
    "commit": "git-cz",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "title": "react-native-spinkit",
  "description": "A collection of animated loading indicators for React Native",
  "main": "index.js",
  "keywords": [
    "react-component",
    "react-native",
    "ios",
    "android",
    "spinkit",
    "spinner",
    "loader",
    "indicator"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/darleikroth/react-native-spinkit.git"
  },
  "author": {
    "username": "maxs15",
    "name": "Maxime Mezrahi",
    "email": "maxime.mezrahi@gmail.com",
    "url": "https://github.com/maxs15"
  },
  "rnpm": {
    "commands": {
      "prelink": "node ./node_modules/@iume/react-native-spinkit/scripts/rnpm-prelink.js"
    }
  },
  "dependencies": {
    "prop-types": "^15.5.8"
  },
  "bugs": {
    "url": "https://github.com/darleikroth/react-native-spinkit/issues"
  },
  "homepage": "https://github.com/darleikroth/react-native-spinkit#readme",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^7.6.1",
    "@commitlint/config-conventional": "^7.6.0",
    "commitizen": "^3.1.1",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^2.3.0",
    "standard-version": "^6.0.1"
  }
}
